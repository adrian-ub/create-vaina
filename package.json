{
  "name": "create-vaina",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.13.0",
  "description": "Scaffolding Your First Project",
  "author": "Adri√°n UB",
  "license": "MIT",
  "funding": "https://github.com/sponsors/adrian-ub",
  "homepage": "https://github.com/adrian-ub/create-vaina#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrian-ub/create-vaina.git"
  },
  "bugs": "https://github.com/adrian-ub/create-vaina/issues",
  "keywords": [
    "scaffold",
    "project"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "create-vaina": "bin/create-vaina.mjs"
  },
  "files": [
    "dist",
    "template-*"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@adrianub/eslint-config": "^0.0.1",
    "@antfu/ni": "^0.23.0",
    "@types/node": "^22.9.0",
    "bumpp": "^9.8.1",
    "eslint": "^9.12.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.13.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
